@{
  Layout="Shared/_Layout.cshtml";
}


<div class="row" id="index">
  <div class="col-md-6">
    <h3>Bands</h3>
    @if (@Model["bands"].Count == 0)
    {
      <p>No bands to display</p>
    }
    <ul>
      @foreach(var band in @Model["bands"])
      {
        <li><a href="/band/@band.GetId()">@band.GetName()</a></li>
      }
    </ul>
  </div>
  <div class="col-md-6">
    <h3>Venues</h3>
    @if (@Model["venues"].Count == 0)
    {
      <p>No venues to display</p>
    }
    <ul>
      @foreach(var venue in @Model["venues"])
      {
        <li><a href="/venue/@venue.GetId()">@venue.GetName()</a></li>
      }
    </ul>
  </div>
</div>
