@{
  Layout="Shared/_Layout.cshtml";
}


<div class="row" id="index">
  <div class="col-md-6">
    <h3>Bands</h3>
    @if (@Model["bands"].Count == 0)
    {
      <p>No bands to display</p>
    }
    <ul>
      @foreach(var band in @Model["bands"])
      {
        <li><a href="/band/@band.GetId()">@band.GetName()</a></li>
      }
    </ul>
  </div>
  <div class="col-md-6">
    <h3>Venues</h3>
    @if (@Model["venues"].Count == 0)
    {
      <p>No venues to display</p>
    }
    <ul>
      @foreach(var venue in @Model["venues"])
      {
        <li><a href="/venue/@venue.GetId()">@venue.GetName()</a></li>
      }
    </ul>
  </div>
  <audio src="/Content/music/Sweet child o mine Oct 13th '16.mp3" autoplay>
  <p>If you are reading this, it is because your browser does not support the audio element.</p>
  <embed src="/Content/music/Sweet child o mine Oct 13th '16.mp3" width="180" height="90" hidden="true" />
  </audio>
</div>
